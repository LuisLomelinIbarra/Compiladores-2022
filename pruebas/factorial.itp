programa factorial { 
	
	entero n;
	entero r[ 2 ];
	entero l[ 3 ][ 2 ];
	flotante g[1][1][1];
	
	funcion entero factorial (entero num){}{
		si(num > 0){
			regresar num * $factorial(num-1);
		}sino{
			regresar 1;
		}
	}

	

	principal funcion
	{
		entero aka,aux,aux2;
		

	}
	{
		n = $factorial(5);
		imprimir("Factorial de n \n");
		imprimir(n);
		aka = 10-n;
		10 / (-aka) ;
		aka = 5;
		aux2 = aka;
		aux = 1;
		mientras(aux <= aux2){
			si (aux2 - aux > 0 ){ 
				aka = aka * (aux2-aux);
			}sino{
				aka = aka * 1;
			}
			aux = aux + 1;
		}
		imprimir("Factorial Secuencial : ",aka);
		
	} 

}
